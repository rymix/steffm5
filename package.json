{
  "name": "steffm5",
  "version": "1.0.0",
  "author": "Steve Arnott",
  "description": "Stef.FM - Funky House and Soul Music - Adventures in Decent Music",
  "license": "MIT",
  "private": false,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "tests/**/*.+(test|spec).+(ts|tsx|js)",
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/*.(test|spec).+(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "components/**/*.{ts,tsx}",
      "hooks/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}",
      "!**/*.test.{ts,tsx}",
      "!**/*.spec.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": ["ts-jest", {
        "tsconfig": {
          "jsx": "react-jsx"
        }
      }]
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  },
  "homepage": "https://github.com/rymix/steffm5.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rymix/steffm5.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vite": "^7.1.10"
  },
  "keywords": [
    "react",
    "react-component",
    "player",
    "audio-player",
    "music-player",
    "mixcloud-player",
    "mixcloud-widget",
    "mixcloud",
    "typescript",
    "audio",
    "music",
    "streaming",
    "playlist"
  ],
  "dependencies": {
    "next": "^15.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
